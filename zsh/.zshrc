alias ctags="/usr/local/bin/ctags  -R  --c-kinds=+p"

alias rm=trash 
trash(){
	if [ -d ~/.trash/$@ ]; then
		/bin/rm -rf ~/.trash/$@
	fi
	mv $@ ~/.trash/
}

